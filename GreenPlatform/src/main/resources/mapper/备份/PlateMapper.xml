    </insert>
    <delete id="delPlateCodeDmz" parameterType="com.greenplatform.model.PlateCodeDmz">
        DELETE FROM plate_code_dmz WHERE cDmlb = #{cDmlb}
        <if test='cDm!=null and cDm!=""'>
