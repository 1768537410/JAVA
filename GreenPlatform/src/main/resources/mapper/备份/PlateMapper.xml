    <!--PlateCodeDmz crud-->
    <select id="selectPlateCodeDmz" parameterType="com.greenplatform.model.PlateCodeDmz" resultType="com.greenplatform.model.PlateCodeDmz">
        SELECT T.* FROM plate_code_dmz T WHERE 1=1
        <if test='cDmlb!=null and cDmlb!=""'>
