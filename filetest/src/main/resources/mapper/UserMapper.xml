<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luo.filetest.dao.UserMapper">

    <insert id="addUser" parameterType="java.util.List">
        insert into bankTb
        (docNumber,account,firstTime,flag,unit,remark,info,moneyOut,moneyIn,timeEnd)
        values
        (
        #{docNumber},
        #{account},
        #{time},
        #{flag},
        #{unit},
        #{remark},
        #{info},
        #{moneyOut},
        #{moneyIn},
        #{Timestamp}
        )
    </insert>
</mapper>
